% mmind-datalog---itertools.ps BEGIN

% CL = class

itertools_product(CH0,CH1,CH2,CH3,CL) :- isa(CH0,CL), isa(CH1,CL), isa(CH2,CL), isa(CH3,CL).    % sz = 1296 = NOC**NOH

% reconciliations with python itertools are mentioned below.

itertools_permutations(CH0,CH1,CH2,CH3,CL) :- itertools_product(CH0,CH1,CH2,CH3,CL),
CH0 != CH1, CH0 != CH2, CH0 != CH3,
CH1 != CH2, CH1 != CH3,
CH2 != CH3.
